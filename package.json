{
  "name": "numbers-only",
  "version": "1.0.3",
  "description": "Javascript event to only allow numbers in an input field",
  "main": "dist",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src/index.js -o ./dist/index.js",
    "test": "./node_modules/jest/bin/jest.js --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfcolon/js-numbers-only.git"
  },
  "keywords": [
    "javascript",
    "events",
    "number",
    "numbers only",
    "form",
    "input",
    "is numeric"
  ],
  "author": "Luis Colon <luiscolon62@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luisfcolon/js-numbers-only/issues"
  },
  "homepage": "https://github.com/luisfcolon/js-numbers-only#readme",
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!dist/**/*.{js}"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "coveralls": "^3.0.6",
    "gulp-uglify": "^3.0.2",
    "jest": "^27.0.5"
  }
}
